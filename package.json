{
 "name": "mocxykit",
 "version": "2.5.1",
 "description": "This is an express middleware that can be used with webpack and vite. Its main function is to visualize the configuration, manage proxies, and mock data.",
 "main": "serverDist/index.js",
 "author": "shunseven",
 "license": "MIT",
 "keywords": [
  "express",
  "proxy",
  "express middleware",
  "mock",
  "middleware",
  "API",
  "dependencies",
  "visualization",
  "visualization configuration",
  "webpack",
  "vite",
  "vite-mock",
  "vite-proxy",
  "webpack-mock",
  "webpack-proxy",
  "webpack-dev-server",
  "vite-dev-server",
  "webpack-dev-server-mock",
  "vite-dev-server-mock"
 ],
 "scripts": {
  "build": "tsc & npm run build:view & npm run copy-files",
  "lint": "eslint .",
  "start": "ts-node server.ts",
  "build:view": "vite build",
  "copy-files": "copyfiles -u 1 'main/**/*.p12' 'serverDist'",
  "test:webpack": "webpack serve --config webpack.test.js",
  "test:vite": "vite --config vite.test.js"
 },
 "dependencies": {
  "@eslint/js": "^9.11.1",
  "@faker-js/faker": "^9.3.0",
