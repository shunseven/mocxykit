  "@types/react": "^18.3.10",
  "@types/react-dom": "^18.3.0",
  "antd": "^5.21.3",
  "http-proxy": "^1.18.1",
  "jsoneditor": "^10.1.0",
  "ngrok": "^5.0.0-beta.2",
  "path-to-regexp": "^8.2.0",
  "react": "^18.3.1",
  "react-dom": "^18.3.1"
 },
 "devDependencies": {
  "@types/http-proxy": "^1.17.15",
  "@vitejs/plugin-react-swc": "^3.5.0",
  "babel-plugin-import": "^1.13.8",
  "copyfiles": "^2.4.1",
  "eslint": "^9.11.1",
  "eslint-plugin-react": "^7.37.0",
